{
  "name": "gitload-cli",
  "version": "1.0.2",
  "description": "Download files, folders, or entire repos from GitHub URLs",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "gitload": "./src/index.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "github",
    "download",
    "cli",
    "git"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waldekmastykarz/gitload.git"
  },
  "files": [
    "src",
    "!src/**/*.test.js"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "archiver": "^7.0.1",
    "chalk": "^5.3.0",
    "commander": "^14.0.2",
    "ora": "^9.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "vitest": "^4.0.16"
  }
}
